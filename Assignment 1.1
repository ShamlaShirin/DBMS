//Employee Table
create table EMPLOYEE(Emp_Id int,Name varchar(20),City varchar(10),Dob date,Adhaar_No int);
desc EMPLOYEE;
+-----------+-------------+------+-----+---------+-------+
| Field     | Type        | Null | Key | Default | Extra |
+-----------+-------------+------+-----+---------+-------+
| Emp_Id    | int         | YES  |     | NULL    |       |
| Name      | varchar(20) | YES  |     | NULL    |       |
| City      | varchar(10) | YES  |     | NULL    |       |
| Dob       | date        | YES  |     | NULL    |       |
| Adhaar_No | int         | YES  |     | NULL    |       |
+-----------+-------------+------+-----+---------+-------+


//Student Table
create table STUDENT(Roll_No int,Name varchar(20) not null,Age int check(Age>0 and Age<99) not null,Address varchar(30),Gender varchar(2) default "m",primary key(Roll_No));
desc STUDENT;
+---------+-------------+------+-----+---------+-------+
| Field   | Type        | Null | Key | Default | Extra |
+---------+-------------+------+-----+---------+-------+
| Roll_No | int         | NO   | PRI | NULL    |       |
| Name    | varchar(20) | NO   |     | NULL    |       |
| Age     | int         | NO   |     | NULL    |       |
| Address | varchar(30) | YES  |     | NULL    |       |
| Gender  | varchar(2)  | YES  |     | m       |       |
+---------+-------------+------+-----+---------+-------+


//Temporary Student Table
create temporary table STUDENT1 select * from STUDENT limit 0;
desc STUDENT1;
+---------+-------------+------+-----+---------+-------+
| Field   | Type        | Null | Key | Default | Extra |
+---------+-------------+------+-----+---------+-------+
| Roll_No | int         | NO   |     | NULL    | NULL  |
| Name    | varchar(20) | NO   |     | NULL    | NULL  |
| Age     | int         | NO   |     | NULL    | NULL  |
| Address | varchar(30) | YES  |     | NULL    | NULL  |
| Gender  | varchar(2)  | YES  |     | m       | NULL  |
+---------+-------------+------+-----+---------+-------+


//Duplicate Table
create table if not exists EMPLOYEE1 select * from EMPLOYEE;
desc EMPLOYEE1;
+-----------+-------------+------+-----+---------+-------+
